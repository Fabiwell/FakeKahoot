<UserControl x:Class="FakeKahoot.Loginform"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FakeKahoot"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Canvas Margin="225,18,225,18" Background="#FF3F2E2E">
            <TextBlock Canvas.Left="136" TextWrapping="Wrap" Text="Register" Canvas.Top="63" Foreground="#FFCCC8C8" FontSize="22" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            <Button x:Name="btnSubmitform" Content="Register" HorizontalAlignment="Center" VerticalAlignment="Top" Width="139" Height="24" Background="#FF675A5A" Foreground="#FFD7D6D6" Canvas.Left="106" Canvas.Top="233"/>
        </Canvas>
        <TextBox x:Name="tbEmail" HorizontalAlignment="Center" Margin="0,142,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="120" Foreground="#FFCBCBCB" Background="#FF6C5F5F" UseLayoutRounding="False"/>

    </Grid>
</UserControl>
