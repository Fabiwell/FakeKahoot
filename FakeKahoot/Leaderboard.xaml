<Window x:Class="FakeKahoot.Leaderboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FakeKahoot"
        mc:Ignorable="d"
        Title="Leaderboard" Height="450" Width="800">
    <Grid Background="#FF3F2E2E">
        <TextBlock HorizontalAlignment="Center" Margin="0,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FFB7B7B7" FontSize="22" Height="30" Text="Leaderboard"/>
        <ListBox x:Name="lbLeaderBoard" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="323,110,323,110"/>

    </Grid>
</Window>
